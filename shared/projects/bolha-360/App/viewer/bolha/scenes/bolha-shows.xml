		url="%SWFPATH%/plugins/gyro2.js"
        enabled="true"
        camroll="true"
        friction="0.0"
        touch_mode="full"
        sensor_mode="1"
        softstart="0.5"
        autocalibration="false"
        onavailable=""
        onunavailable=""
